{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "import/extensions": "off",
        "semi": 2,
        "no-console": "off",
        "linebreak-style": "off",
        "operator-assignment": "off",
        "no-use-before-define": "off",
        "prefer-destructuring": "off"
    }
}