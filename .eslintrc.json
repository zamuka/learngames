{
    "env": {
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", 2],
        "import/extensions": "off",
        "semi": 2,
        "no-console": "off",
        "linebreak-style": "off",
        "operator-assignment": "off",
        "no-use-before-define": "off",
        "prefer-template": "off",
        "prefer-destructuring": "off",
        "prefer-const": "off",
        "no-unused-vars": "off",
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "only-multiline",
            "imports": "never",
            "exports": "never",
            "functions": "never"
        }]
    }
}